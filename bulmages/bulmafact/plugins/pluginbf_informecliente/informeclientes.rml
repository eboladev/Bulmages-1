		<image x="3cm" y="26cm" width="17cm" file="logo.jpg"/>

		<!-- EL PIE DE PAGINA -->
		<setFont name="Helvetica" size="8"/>		
		<fill color="(0,0,0)"/>	
		<stroke color="(0,0,0)"/>
		<drawString x="7.3cm" y="1.2cm">www.iglues.org -- info@iglues.org --</drawString>

		<fill color="(0,0,0)"/>	
		<stroke color="(0,0,0)"/>
		<setFont name="Helvetica" size="14"/>
		<drawString x="1.5cm" y="24.5cm">INFORME DE CLIENTES</drawString>

		<!-- FRAME DE CONTENIDOS -->
		<frame id="column" x1="1cm" y1="2cm" width="19cm" height="22cm"/>
		</pageGraphics>
	</pageTemplate>	
</template>
